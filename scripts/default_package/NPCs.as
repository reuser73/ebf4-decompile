      public static function countQuests() : int
      {
         var _loc2_:NPC = null;
         var _loc1_:int = 0;
         for each(_loc2_ in questList)
         {
            if(!_loc2_.flag)
            {
               _loc1_++;
            }
         }
         if(_loc1_ >= 10)
         {
            Medals.unlock(Medals.NPChelper);
         }
         if(_loc1_ >= 20)
         {
            Medals.unlock(Medals.NPCservant);
         }
         if(_loc1_ >= 30)
         {
            Medals.unlock(Medals.NPCslave);
         }
         return _loc1_;
      }
